{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "dev": {
      "cache": false,
      "persistent": true,
      "dependsOn": ["generate", "^build"]
    },
    "export": {},
    "storybook": { "cache": false, "persistent": true },
    "lint": {},
    "generate": { "outputs": [".graphclient"] },
    "build": {
      "cache": false,
      "outputs": [".next/**"],
      "dependsOn": ["generate", "^build"]
    },
    "dev-share": {
      "persistent": true
    },
    "build-share": {
      "outputs": [".next/**"]
    },
    "build-storybook": {
      "outputs": ["storybook-static/**"]
    }
  }
}
